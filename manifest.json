{
  "name": "HappyTab",
  "description": "A new page extension",

  "manifest_version": 2,
  "version": "0.0.1",

  "chrome_url_overrides" : {
    "newtab": "html/happytab.html"
  },

  "options_page": "html/options.html",

  "icons": {
    "64": "assets/pineapple-64.png",
    "128": "assets/pineapple-128.png"
  },

  "permissions": ["storage", "topSites", "bookmarks"]
}
